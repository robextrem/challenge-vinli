{
  "name": "luqero",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:development": "vue-tsc && vite build --mode development",
    "build:production": "vue-tsc && vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "stylelint": "stylelint '**/*.scss'",
    "down": "cross-env VITE_APP_MANTAINANCE_MODE=\"on\" yarn dev",
    "serve": "cross-env VITE_APP_MODE=\"production\" yarn dev",
    "test": "vitest --environment jsdom"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.12.19"
  },
  "dependencies": {
    "@pinia/testing": "^0.1.3",
    "@popperjs/core": "^2.11.8",
    "@types/jsdom": "^21.1.1",
    "@vue/test-utils": "^2.4.1",
    "animate.css": "^4.1.1",
    "axios": "^1.4.0",
    "bulma": "^0.9.4",
    "bulma-switch": "^2.0.4",
    "bulma-toast": "^2.4.2",
    "cross-env": "^7.0.3",
    "happy-dom": "^10.8.0",
    "intl-tel-input": "^18.1.8",
    "pinia": "^2.1.4",
    "pinia-plugin-persist": "^1.0.0",
    "pusher-js": "^8.3.0",
    "simple-vue-validator": "^0.16.0",
    "simple-vue3-validator": "^1.0.0-beta.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.15.1",
    "jsdom": "^22.1.0",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-scss": "^5.0.1",
    "stylelint-use-nesting": "^4.1.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-stylelint": "^4.3.0",
    "vitest": "^0.34.1",
    "vue-tsc": "^1.8.3"
  }
}
